  </servlet>
  
  <servlet-mapping>
  <servlet-name>mHomePage</servlet-name>
  <url-pattern>/managerhome</url-pattern>
  </servlet-mapping>
  
    <servlet>
  <servlet-name>eHomePage</servlet-name>
  <servlet-class>com.revature.projectr.controller.EmployeeHomePageServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
