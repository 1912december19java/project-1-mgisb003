  <servlet-class>com.revature.projectr.controller.EmployeeRegisterServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>register</servlet-name>
  <url-pattern>/register</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>eLogin</servlet-name>
  <servlet-class>com.revature.projectr.controller.EmployeeLoginServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>eLogin</servlet-name>
  <url-pattern>/employee</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>mLogin</servlet-name>
